{
	"name": "ClonedCSVFiles",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureBlobStorage",
			"type": "LinkedServiceReference"
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@concat('Cloned_', item().name, '.csv')",
					"type": "Expression"
				},
				"folderPath": "output",
				"container": "thaicontainer"
			},
			"columnDelimiter": "|",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}